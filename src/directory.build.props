<Project>
  <PropertyGroup>
    <LangVersion>latest</LangVersion>
	<Authors>David Hary</Authors>
    <Company>Integrated Scientific Resources</Company>
    <Copyright>(c) 2013 Integrated Scientific Resources, Inc. All rights reserved.</Copyright>
    <NeutralLanguage>en-US</NeutralLanguage>
    <GenerateAssemblyInfo>true</GenerateAssemblyInfo>
    <AutoGenerateBindingRedirects>false</AutoGenerateBindingRedirects>
    <!-- NuGet -->
    <PackageProjectUrl>https://bitbucket.org/davidhary/dn.UnitsAmounts</PackageProjectUrl>
	<PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
	<RepositoryUrl>$(PackageProjectUrl)</RepositoryUrl>
	<RepositoryType>Git</RepositoryType>
    <PackageLicenseExpression>The MIT License</PackageLicenseExpression>
    <PackageIcon>favicon.ico</PackageIcon>
  </PropertyGroup>
  
  <ItemGroup>
    <None Include="..\..\Items\favicon.ico">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
      <Visible>false</Visible>
    </None>
  </ItemGroup>
  
  <Import Project="Items\Version.build.props" />
    
</Project>

